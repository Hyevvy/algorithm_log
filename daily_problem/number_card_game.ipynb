   "source": [
    "n, m = map(int, input().split())\n",
    "result = 0\n",
    "\n",
    "for i in range(m):\n",
    "    data = list(map(int, input().split()))\n",
    "    min_value = 9999 # 최솟값은 각 행마다 새롭게 초기화되어야 한다.\n",
    "    for element in data:\n",
    "        min_value = min(min_value, element)\n",
    "    result = max(result, min_value)\n",
    "\n",
    "print(result)"
   ]
